karmem app @packed(true) @golang.package(`interop`);


struct Annotation inline {
    Key []char;
    Val []char;
}

struct Message table {
    ID            []char;
    Source        []char;
    Type          []char;
    Subject       []char;
    ContentType   []char;
    ContentSchema []char;
    Time          int64;
    Content       []byte;
    Annotations   []Annotation;
}